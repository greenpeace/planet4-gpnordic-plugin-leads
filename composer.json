{
  "name": "greenpeace/greenpeace-leads",
  "description": "Greenpeace Nordic Leads Plugin with ACF fileds",
  "type": "wordpress-plugin",
  "minimum-stability" : "stable",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Simma Lugnt",
      "email": "hello@simmalugnt.se"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3.2",
    "wp-coding-standards/wpcs": "^1.1.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.0",
    "phpunit/phpunit": "^6.2"
  },
  "require": {
    "google/cloud-storage": "^1.23"
  }
}
